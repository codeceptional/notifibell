{
  "name": "aws_sns",
  "version": "1.0.0",
  "description": "POC on AWS SNS",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node --files ./src/index.ts",
    "compile": "rimraf ./dist/ && tsc -b ./tsconfig.compile.json",
    "build": "tsc -b ./tsconfig.compile.json",
    "rmdist": "rimraf ./dist/",
    "test": "jest --runInBand --forceExit",
    "typecheck": "tsc -b",
    "lint:check": "eslint .",
    "lint": "eslint . --fix",
    "format": "cross-env prettier --write 'src/**/*.{ts,tsx}'",
    "format:check": "cross-env prettier --check 'src/**/*.{ts,tsx}'",
    "structure:check": "node ./.code-standards/enforce-file-name.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashubisht/clustered-express.git"
  },
  "author": "ashubisht",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ashubisht/clustered-express/issues"
  },
  "homepage": "https://github.com/ashubisht/clustered-express#readme",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.609.0",
    "@aws-sdk/client-sns": "^3.609.0",
    "aws-sdk": "^2.1618.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "express": "^4.19.2",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/express": "^4.17.21",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "sinon": "^17.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}
